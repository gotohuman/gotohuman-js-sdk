{
  "name": "gotohuman",
  "version": "0.2.6",
  "description": "gotoHuman.com - The easiest way to review AIâ€‘generated content, give approvals, or provide input to your AI workflows.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "shx rm -rf ./dist",
    "build": "npm run clean && tsup src/index.ts --format esm,cjs --dts",
